{
  "name": "@tuilder/ws-client",
  "version": "0.0.1",
  "description": "Websocket Client Boilerplate",
  "typings": "./lib/index.d.ts",
  "module": "./lib/index.mjs",
  "es2022": "./lib/index.mjs",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.mjs",
      "node": "./lib/index.mjs",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "es2022": "./lib/index.mjs"
    }
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "npm run build -- --watch src",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tuilder-Web-Services/ws-client.git"
  },
  "author": "Emmanuel Higgins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tuilder-Web-Services/ws-client/issues"
  },
  "homepage": "https://github.com/Tuilder-Web-Services/ws-client#readme",
  "devDependencies": {
    "@types/websocket": "^1.0.5",
    "@types/ws": "^8.5.4",
    "ts-loader": "^9.4.2",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "nanoid": "^4.0.1",
    "rxjs": "^7.8.0"
  }
}
